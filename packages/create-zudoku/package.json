{
  "name": "create-zudoku",
  "version": "0.61.5",
  "keywords": [
    "react",
    "zudoku"
  ],
  "description": "Create Zudoku docs site with one command",
  "repository": {
    "type": "git",
    "url": "https://github.com/zuplo/zudoku",
    "directory": "packages/create-zudoku"
  },
  "author": "Zuplo <support@zuplo.com>",
  "license": "MIT",
  "bin": {
    "create-zudoku": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ncc build ./index.ts -w -o dist/",
    "build": "rm -rf ./dist && ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register && cp -r templates ./dist/"
  },
  "devDependencies": {
    "@types/async-retry": "1.4.2",
    "@types/cross-spawn": "6.0.6",
    "@types/node": "catalog:",
    "@types/prompts": "2.4.9",
    "@types/tar": "6.1.13",
    "@types/validate-npm-package-name": "4.0.2",
    "@vercel/ncc": "0.38.3",
    "async-retry": "1.3.1",
    "ci-info": "4.3.0",
    "commander": "14.0.0",
    "conf": "13.1.0",
    "cross-spawn": "7.0.6",
    "fast-glob": "3.3.3",
    "picocolors": "1.1.1",
    "prompts": "2.4.2",
    "tar": "7.4.3",
    "update-check": "1.5.4",
    "validate-npm-package-name": "6.0.2"
  },
  "engines": {
    "node": ">=20"
  }
}
