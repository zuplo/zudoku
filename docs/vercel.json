{
  "framework": null,
  "devCommand": "npx nx run docs:dev",
  "buildCommand": "npx nx run docs:build",
  "outputDirectory": "dist",
  "installCommand": "pnpm install",
  "routes": [
    {
      "src": "/",
      "status": 308,
      "headers": {
        "Location": "/docs/introduction"
      }
    },
    {
      "src": "/docs",
      "status": 308,
      "headers": {
        "Location": "/docs/introduction"
      }
    },
    {
      "src": "/introduction",
      "dest": "/introduction.html"
    },
    {
      "src": "/docs/(.*)",
      "dest": "/docs/$1.html"
    }
  ]
}
