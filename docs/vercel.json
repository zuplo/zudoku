{
  "framework": "other",
  "devCommand": "npx nx run docs:dev",
  "buildCommand": "npx nx run docs:build",
  "outputDirectory": "dist",
  "installCommand": "pnpm install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/documenation",
      "permanent": true
    }
  ]
}
