{
  "framework": null,
  "devCommand": "npx nx run docs:dev",
  "buildCommand": "npx nx run docs:build && mv dist _dist && mkdir -p dist/docs && mv _dist/* dist/docs && rm -r _dist",
  "outputDirectory": "dist",
  "installCommand": "pnpm install",
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/",
      "destination": "/docs/introduction",
      "permanent": false
    },
    {
      "source": "/docs",
      "destination": "/docs/introduction",
      "permanent": false
    },
    {
      "source": "/docs/getting-started",
      "destination": "/docs/introduction",
      "permanent": false
    }
  ]
}
